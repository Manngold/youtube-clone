extends layouts/main.pug
include mixins/videoBlock

block content
    .search__header
        h3 Result about #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Founded
        else
            each video in videos
                +videoBlock({
                    title: video.title,
                    views: video.views,
                    videoFile: video.videoFile,
                    id: video.id
                })